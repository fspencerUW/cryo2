FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0";
    object      alpha.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];
internalField   uniform 1;

boundaryField
{
    inlet
    {
        type            fixedValue;        
        value           uniform 0;  // LNâ‚‚ enters here
    }

    outlet
    {
        type            zeroGradient;        
    }

    walls
    {
        type            alphaContactAngle;
        contactAngleProperties
        {
            liquid          { theta0 90; }
            vapor           { theta0 90; }            
        }
        value           uniform 0;
    }
}



