FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0";
    object      alpha.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];
internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1;  // LNâ‚‚ enters here
    }

    outlet
    {
        type            zeroGradient;
    }

    walls
    {
        type            alphaContactAngle;
        contactAngleProperties
        {
            liquid          {theta0 90; }
            vapor           {theta0 90; }            
        }
        value           uniform 0;
        
    }
}



