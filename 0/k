FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];   // m²/s²

internalField   uniform 1e-6;       // Small initial turbulence level


boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1e-6;
    }

    outlet
    {
        type            zeroGradient;
    }

    walls
    {
        type            kqRWallFunction;
        value           uniform 1e-6;
    }
}



// ************************************************************************* //
