FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      physicalProperties.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phase           air;

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          eConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleInternalEnergy;
}

mixture
{
    specie
    {
        molWeight   28.9;
    }
    thermodynamics
    {
        Cv          720;   // Cv = Cp - R = 1007 - 287 = 720 J/kg/K
        hf          0;
    }
    transport
    {
        mu          1.84e-05;
        Pr          0.7;
    }
    equationOfState
    {
        type        rhoConst;
        rho         1.225;   // kg/mÂ³ - constant air density at standard conditions
    }
}

