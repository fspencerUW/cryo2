FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      physicalProperties.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type          heRhoThermo;
  mixture         pureMixture;
    transport     const; 
  thermo          eConst;
  equationOfState rhoConst;         // liquid LN2 ~ incompressible
    specie        specie;
  energy          sensibleInternalEnergy;
}

mixture
{

  specie
  {
    molWeight       28.0134; // Molar mass of nitrogen (N2)
  }    

  thermodynamics
  {
    Cv          2000;      // J/kg/K (using eConst -> Cv)
    hf          0;
  }

  transport
  {
  mu              1.6e-4; // Dynamic viscosity [kg/(mÂ·s)] ~ LN2 at 77 K
  Pr              0.85;   // Prandtl number at 77 K
  }    

  equationOfState
  {
      type          rhoConst;
      rho           804;     // kg/m3
  }
}


